@startuml
skinparam linetype ortho
skinparam backgroundColor #1E1E1E
skinparam defaultTextAlignment center
skinparam ArrowColor #B0C4DE
skinparam shadowing false
skinparam FontColor #FFFFFF
skinparam classFontColor #FFFFFF
skinparam classAttributeFontColor #FFFFFF
skinparam classMethodFontColor #FFFFFF

skinparam class {
  BackgroundColor #2D2D2D
  BorderColor #8A8A8A
  FontColor #E0E0E0
}

!include Validator.puml

interface StringNameValidator extends Validator {
    + boolean isValid(HasStringName hasStringName)
}

class NonBlankStringNameValidator implements StringNameValidator {
    + boolean isValid((HasStringName hasStringName)
    - boolean hasNonBlankName(HasStringName hasStringName)
    - boolean hasBlankName(HasStringName hasStringName)
}
@enduml