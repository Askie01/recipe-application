interface LongIdValidator extends Validator {
    + boolean isValid(HasLongId hasLongId))
}

class PositiveLongIdValidator implements LongIdValidator {
    + boolean isValid(HasLongId hasLongId)
    - boolean hasPositiveId(HasLongId hasLongId)
}
@enduml